        else {
            
            var timeDelay = 10; 

            var Pix = new Array
                ("_ui/img/red-reveal-off.png"
                ,"_ui/img/red-reveal-on.png"
            );

            var howMany = Pix.length;
            timeDelay *= 1000;
            var PicCurrentNum = 0;
            var PicCurrent = new Image();
            PicCurrent.src = Pix[PicCurrentNum];
            
            function startPix() {
                setInterval("showHideRed()", timeDelay);
            }
            
            function showHideRed() {
                PicCurrentNum++;
                
                if (PicCurrentNum == howMany) {
                    PicCurrentNum = 0;
                }
               
                PicCurrent.src = Pix[PicCurrentNum];
                
                $('see-it-home').setStyle({
                    width: '359px',
                    height: '264px',
                    background: 'transparent url(' + PicCurrent.src + ')',
                    position: 'relative',
                    top: '24px',
                    left: '0px'  
                });
                
                $('see-it-home').observe('mouseover', function(event) {
                    this.setStyle({background: 'transparent url(_ui/img/red-reveal-on.png)'});  
                });
                $('see-it-home').observe('mouseout', function(event) {
                    this.setStyle({background: 'transparent url(_ui/img/red-reveal-off.png)'});
                });
            }
        }
	</script> 
